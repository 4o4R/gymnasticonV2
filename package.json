{
  "name": "gymnasticon",
  "version": "2.0.0",
  "description": "Enables the Flywheel Home Bike to work with Zwift and other training apps.",
  "engines": { "node": "14.x" },
  "type": "module",
  "exports": { ".": "./src/index.js" },
  "bin": { "gymnasticon": "./src/app/cli.js" },
  "scripts": {
    "start": "node src/app/cli.js",
    "build": "node scripts/rebuild-native.cjs",
    "postinstall": "node scripts/rebuild-native.cjs",
    "test": "node scripts/run-tests.mjs",
    "dev": "node src/app/cli.js --bike bot --bot-power 150 --bot-cadence 80",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "imports": {
    "#noble": "./stubs/noble.js",
    "#bleno": "./stubs/bleno.js",
    "#ant": "./stubs/gd-ant-plus.js",
    "#serialport": "./stubs/serialport.js",
    "#parser-delimiter": "./stubs/parser-delimiter.js",
    "#debug": "./stubs/debug.js"
  },
  "files": ["src"],
  "author": "<ptx2@ptx2.net>",
  "license": "MIT",
  "dependencies": {
    "@abandonware/bleno": "0.5.1-3",
    "@abandonware/noble": "1.9.2-10",
    "bindings": "^1.5.0",
    "debug": "4.3.1",
    "nan": "^2.17.0",
    "node-gyp": "^9.0.0",
    "prebuild-install": "^7.1.1",
    "yargs": "15.3.1",
    "gd-ant-plus": "0.0.29",
    "serialport": "9.0.1",
    "usb": "1.6.2",
    "bluetooth-hci-socket": "npm:@abandonware/bluetooth-hci-socket@0.5.3-12",
    "tape": "5.0.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "sinon": "9.2.0",
    "node-addon-api": "^4.3.0"
  },
  "optionalDependencies": {
    "@abandonware/bluetooth-hci-socket": "0.5.3-12"
  },
  "overrides": {
    "bluetooth-hci-socket": "@abandonware/bluetooth-hci-socket"
  }
}
